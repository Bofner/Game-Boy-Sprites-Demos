;Header byte follows this format:
;7:     1 = TILE, 0 = MAP
;6:     1 = Uncompressed
;5 & 4: 00 = SCRN, 01 = TALL
;       10 = WIDE, 11 = FULL
;0-3: Unused but set to 1
.DB %00001111
;Compressed tile data in the form $RunLength + $TileID written as a word ($RLID).
.DW $0100 $0101 $0102 $0103 $0104 $0105 $0106 $0202 $0107 $0108 $0105 $0107 $0202 $0109 $010A $0109 
.DW $010A $010B $010C $010D $010E $040F $0110 $0111 $0112 $0213 $0114 $0211 $0115 $0116 $0115 $0116 
.DW $0117 $0718 $0119 $0D18 $011A $0118 $011B $0118 $011C $0418 $011C $0218 $011C $0118 $0119 $0418 
.DW $0119 $0B18 $011A $0418 $011B $0518 $011C $0418 $011A $0A18 $011A $1018 $011A $0718 $0119 $0218 
.DW $011C $0218 $0119 $1518 $011C $0418 $011B $0218 $011A $011C $0E18 $0119 $0918 $011A $0318 $011B 
.DW $0318 $011B $0418 $011C $1618 $0119 $0118 $011B $0318 $011C $0318 $0119 $0418 $011A $0C18 $011A 
.DW $0A18 $011B $0518 $0119 $0618 $011B $0119 $0418 $011A $0318 $011C $0618 $011C $0718 $0119 $011C 
.DW $1418 $0119 $0418 $011A $0118 $011B $0218 $011A $0218 $011C $0218 $011A $0218 
;Terminator word is $0000 since we can't have a run length of length 0.
.DW $0000
